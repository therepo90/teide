{
  "name": "teide-lp",
  "version": "1.0.0",
  "description": "",
  "main": "index.html",
  "scripts": {
    "start": "rm -rf dist && parcel index.html",
    "start:feonly": "rm -rf dist && NODE_ENV=frontend parcel index.html",
    "build": "rm -rf dist && parcel build index.html --experimental-scope-hoisting",
    "build:prod": "rm -rf docs/* && cross-env NODE_ENV=production parcel build index.html --no-source-maps --no-content-hash -t browser -d docs --public-url ./ && cp robots.txt docs/",
    "deploy": "npm run build:prod && git add . && git commit -m \" zz\" && git push -u  origin && git push -u staging"
  },
  "engines": {
    "node": "v16"
  },
  "browserslist": "> 1%, last 2 versions, not dead",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@parcel/transformer-less": "^2.12.0",
    "cross-env": "^7.0.3",
    "eslint": "8",
    "less": "^4.2.0",
    "parcel-bundler": "^1.12.5",
    "parcel-plugin-glsl": "^0.1.0",
    "parcel-plugin-inline-source": "^1.0.0"
  }
}
